{
    "openapi": "3.0.0",
    "info": {
        "title": "",
        "description": "",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "/api",
            "description": "API сервер"
        }
    ],
    "paths": {
        "/available-cars": {
            "get": {
                "tags": [
                    "Available Cars"
                ],
                "summary": "Получить доступные автомобили по ID пользователя",
                "description": "Возвращает список автомобилей, доступных пользователю на выбранное время",
                "operationId": "getAvailableCarsByUserId",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "ID пользователя",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    },
                    {
                        "name": "start_time",
                        "in": "query",
                        "description": "Время начала брони",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2025-11-10T09:00:00"
                        }
                    },
                    {
                        "name": "end_time",
                        "in": "query",
                        "description": "Время окончания брони",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2025-11-10T18:00:00"
                        }
                    },
                    {
                        "name": "model",
                        "in": "query",
                        "description": "Фильтр по модели автомобиля",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "category",
                        "in": "query",
                        "description": "Фильтр по категории комфорта",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Список доступных автомобилей",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer"
                                            },
                                            "car_model": {
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "comfort_category": {
                                                        "properties": {
                                                            "name": {
                                                                "type": "string"
                                                            }
                                                        },
                                                        "type": "object"
                                                    }
                                                },
                                                "type": "object"
                                            },
                                            "driver": {
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": []
            }
        }
    },
    "tags": [
        {
            "name": "Available Cars",
            "description": "Available Cars"
        }
    ]
}